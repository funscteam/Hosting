while True:
    paswword = input("masukan paswword yang kuat (minimal 8 huruf, simbo, angka atau campuranya: ")
    if len(paswword) >= 8 and any(char.isalpha()for char in paswword)  and any(char.isdigit() for char in paswword) and any(not char.isalnum() for char in paswword):
        break
    else:
        print("Password tidak memenuhi syarat.")
print("passwor diterrima")
